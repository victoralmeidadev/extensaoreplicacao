{
  "name": "Extensão Replicação",
  "version": "1.0",
  "description": "Extensão Replicação",
  "manifest_version": 2,
  "content_scripts": [
    {
      "matches": ["<all_urls>"],
      "js": ["content.js"]
    }
  ],
  "browser_action": {
    "default_popup": "index.html",
    "default_title": "Replicação",
    "default_icon": "replicacao.png"
  },
  "permissions": ["tabs"]
}
